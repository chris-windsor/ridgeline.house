---
const architecturalPagesData = await fetch(
  "https://ridgeline-house.s3.us-west-2.amazonaws.com/architecturalpages"
);
const architecturalPagesText = await architecturalPagesData.text();
const architecturalPageList = architecturalPagesText.split(",");
---

{
  architecturalPageList.map((page) => (
    <img
      src={`https://ridgeline-house.s3.us-west-2.amazonaws.com/${page}.jpg`}
      alt={page}
    />
  ))
}

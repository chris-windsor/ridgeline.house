---
const inspirationPagesData = await fetch(
  "https://ridgeline-house.s3.us-west-2.amazonaws.com/inspirationpages"
);
const inspirationPagesText = await inspirationPagesData.text();
const inspirationPageList = inspirationPagesText.split(",");
---

{
  inspirationPageList.map((page) => (
    <img
      src={`https://ridgeline-house.s3.us-west-2.amazonaws.com/inspiration/${page}.png`}
      alt={`inspiration ${page}`}
    />
  ))
}
